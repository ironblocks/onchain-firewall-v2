{
  "name": "venn-core",
  "version": "2.0.0",
  "private": true,
  "description": "Venn Core Contracts",
  "scripts": {
    "install": "npx husky install",
    "build": "hardhat compile",
    "test": "npx hardhat test $(find './test' -type f -iname '*test.ts' -not -path './test/fork/*')",
    "test-fork": "npx hardhat test $(find './test' -type f -iname '*test.ts' -path './test/fork/*')",
    "test-all": "npx hardhat test",
    "coverage": "npx hardhat coverage --solcoverjs ./.solcover.ts",
    "private-network": "npx hardhat node --trace",
    "generate-types": "hardhat typechain",
    "solhint-check": "solhint \"./contracts/**/*.sol\"",
    "lint-fix": "npm run lint-sol-fix && npm run lint-ts-fix && npm run lint-json-fix && npm run solhint-check",
    "lint-json-fix": "prettier --write \"./**/*.json\"",
    "lint-ts-fix": "prettier --write \"./**/*.ts\"",
    "lint-sol-fix": "prettier --write \"contracts/**/*.sol\""
  },
  "devDependencies": {
    "@ironblocks/omcl": "^1.0.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.12",
    "@nomicfoundation/hardhat-verify": "^2.0.13",
    "@solarity/hardhat-markup": "^1.0.10",
    "@solarity/hardhat-migrate": "^3.0.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^5.2.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.14.1",
    "bignumber.js": "^9.3.0",
    "chai": "^4.5.0",
    "dotenv": "^16.5.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^6.13.6",
    "hardhat": "^2.23.0",
    "hardhat-abi-exporter": "^2.11.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^2.2.3",
    "hardhat-tracer": "^3.1.0",
    "husky": "^9.1.7",
    "mocha": "^10.8.2",
    "pinata": "^2.2.1",
    "prettier": "^3.5.3",
    "prettier-plugin-solidity": "^1.4.2",
    "solhint": "^5.0.5",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "^0.8.15",
    "toolog": "^1.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "4.9.3",
    "@openzeppelin/contracts-upgradeable": "4.9.3",
    "@openzeppelin/hardhat-upgrades": "^3.9.0"
  },
  "keywords": [
    "ethereum",
    "smart-contracts",
    "hardhat",
    "firewall",
    "security"
  ],
  "bugs": {
    "url": "https://github.com/ironblocks/onchain-firewall/issues"
  },
  "homepage": "https://github.com/ironblocks/onchain-firewall#readme",
  "license": "SEE LICENSE IN LICENSE",
  "author": "IRONBLOCKS LTD",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ironblocks/onchain-firewall",
    "directory": "packages/onchain-firewall"
  }
}